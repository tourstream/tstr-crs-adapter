{
  "name": "ubp-crs-adapter",
  "version": "0.0.12",
  "description": "This library provides connections to different travel CRSs. It also let you read and write data from/to them.",
  "main": "dist/ubpCrsAdapter.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "rimraf build && webpack --config webpack.config.dev.js && cp build/ubpCrsAdapter.js tests/manual/assets/ubpCrsAdapter.js",
    "build:prod": "rimraf dist && webpack --config webpack.config.prod.js",
    "test": "karma start karma.config.js --single-run",
    "preversion": "npm test",
    "version": "npm run build && cp -R build/* dist/ && git add -A dist tests/manual",
    "postversion": "git push && git push --tags",
    "serve": "npm run build:dev && ws --https --hostname localhost --port 1337 --directory ./tests/manual --static.index crsTest.html"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:tourstream/tstr-crs-adapter.git"
  },
  "keywords": [
    "UBP",
    "CRS",
    "travel",
    "toma",
    "cets",
    "sabre"
  ],
  "author": "Steve Reichenbach",
  "license": "MIT",
  "homepage": "https://github.com/tourstream/tstr-crs-adapter#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "inject-loader": "^3.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.1",
    "karma": "^1.6.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "loader-utils": "^1.1.0",
    "local-web-server": "^2.2.4",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "fast-xml-parser": "^2.4.1",
    "moment": "^2.18.1",
    "penpal": "^2.6.0",
    "xml2js": "^0.4.17"
  }
}
